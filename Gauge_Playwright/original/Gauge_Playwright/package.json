{
  "name": "ETDR-ts-test-implementation",
  "description": "Implementations of ETDR BDD test setps and Playwright integration with Gauge",
  "scripts": {
    "test": "gauge run -p -n 6 --tags 'ready & playwright' -i specs/ -e ",
    "rerun": "gauge run --failed"
  },
  "dependencies": {
    "@playwright/test": "1.19.1",
    "playwright": "1.19.1",
    "@getgauge/cli": "1.4.3",
    "gauge-ts": "0.1.0",
    "randomstring": "1.2.2",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/randomstring": "1.1.8"
  }
}
